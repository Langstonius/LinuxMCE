#!/bin/bash
# vim:set ft=sh:

set -e 
. ./filesd-parms.sh
. /usr/pluto/bin/Network_Parameters.sh

File="/etc/cron.daily/synctime"
Content="usr/sbin/ntpdate dcerouter"

