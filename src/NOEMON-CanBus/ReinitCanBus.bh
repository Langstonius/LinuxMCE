#! /bin/bash

myCmd=776
myObj=129
myParam="CANBUS"

if [[ "$#" -ge 1 ]]; then
  myObj=$1
  echo "Set obj type: " $1
fi

if [[ "$#" -ge 2 ]]; then
  myParam=$2
  echo "Set my param:" $2
fi

/usr/pluto/bin/MessageSend localhost 0 $myObj 1 $myCmd 51 $myParam
echo /usr/pluto/bin/MessageSend localhost 0 $myObj 1 $myCmd 51 $myParam

