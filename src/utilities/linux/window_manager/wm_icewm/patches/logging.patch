+#include <sys/time.h>
+#include <time.h>
+void log_time_stamp()
+{
+    struct timeval tv;
+    gettimeofday(&tv, NULL);
+    struct tm t;
+    localtime_r(&tv.tv_sec, &t);
+    char s[128];
+    memset(&s, 0, sizeof(s));
+    strftime(s, sizeof(s), "%H:%M:%S", &t);
+    fprintf(stderr, "%s.%06d ", s, tv.tv_usec);
+}
