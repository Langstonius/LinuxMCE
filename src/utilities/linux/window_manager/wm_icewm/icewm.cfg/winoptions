# This is an example for IceWM's window options file.
#
# Place your variants in /etc/X11/icewm or in $HOME/.icewm
# since modifications to this file will be discarded when you
# (re)install icewm.

# initial window geometry
pluto-xine-playback-window.pluto-xine-playback-window.geometry: 1x1+12345+12345

# never focus
pluto-xine-playback-window.pluto-xine-playback-window.noFocusOnAppRaise: 1

# always focus
Orbiter.Orbiter.ignoreNoFocusHint: 1
SDL_App.SDL_App.ignoreNoFocusHint: 1

# EOF
